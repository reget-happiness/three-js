ource":"main/main04.js","original":{"line":46,"column":34}},{"generated":{"line":48,"column":2},"source":"main/main04.js","original":{"line":47,"column":4}},{"generated":{"line":49,"column":2},"source":"main/main04.js","original":{"line":48,"column":4},"name":"requestAnimationFrame"},{"generated":{"line":49,"column":23},"source":"main/main04.js","original":{"line":48,"column":25}},{"generated":{"line":49,"column":24},"source":"main/main04.js","original":{"line":48,"column":26},"name":"render"},{"generated":{"line":49,"column":30},"source":"main/main04.js","original":{"line":48,"column":32}},{"generated":{"line":49,"column":31},"source":"main/main04.js","original":{"line":48,"column":33}},{"generated":{"line":50,"column":0},"source":"main/main04.js","original":{"line":49,"column":0}},{"generated":{"line":51,"column":0},"source":"main/main04.js","original":{"line":51,"column":0},"name":"render"},{"generated":{"line":51,"column":6},"source":"main/main04.js","original":{"line":51,"column":6}},{"generated":{"line":51,"column":8},"source":"main/main04.js","original":{"line":51,"column":8}}],"sources":{"main/main04.js":"import * as THREE from 'three'\r\n\r\n// 3D物体的移动\r\n\r\n// 导入轨道控制器\r\nimport { OrbitControls } from 'three/examples/jsm/controls/OrbitControls'\r\n\r\n// 基础内容\r\nconst scene = new THREE.Scene();  //创建场景\r\nconst camera = new THREE.PerspectiveCamera(75, window.innerWidth/window.innerHeight, 0.1, 1000);   //创建相机 (事业角度, 长宽比, 近端, 远端)\r\ncamera.position.set(0, 0, 10); // 设置相机位置(x, y, z)\r\nscene.add(camera) //场景添加相机\r\n\r\n\r\n// 添加物体\r\nconst box = new THREE.BoxGeometry()  // 创建几何体\r\nconst cubeMaterial = new THREE.MeshBasicMaterial({ color: 0xffff00 }) //设置材质\r\nconst cube = new THREE.Mesh(box, cubeMaterial) // 根据几何体和材质创建物体\r\n\r\n// cube.position.set(5, 0, 0) // 修改物体的位置   === cube.position.x = 5\r\n// cube.scale.set(3, 2, 1) // 修改物体的缩放  === cube.scale.x = 5\r\ncube.rotation.set(Math.PI / 4, 0, 0, \"YZX\") // 物体的旋转(默认XYZ, order: 'YZX') === cube.rotation.x = 30\r\nscene.add(cube) // 将物体添加到场景中\r\n\r\n\r\n// 初始化渲染器\r\nconst renderer = new THREE.WebGLRenderer();\r\nrenderer.setSize(window.innerWidth, window.innerHeight) // 设置渲染器的大小\r\ndocument.body.appendChild(renderer.domElement); // 将渲染器添加到body里\r\n\r\n// 使用渲染器 通过相机将场景渲染出来\r\n// renderer.render(scene, camera);\r\n\r\n\r\n// 创建轨道控制器\r\nconst controls = new OrbitControls(camera, renderer.domElement);\r\n\r\n// 添加坐标轴辅助器  (size: 默认长度1)\r\nconst axesHelper = new THREE.AxesHelper(5)  \r\nscene.add(axesHelper);\r\n\r\nfunction render () {\r\n    cube.position.x += 0.01; // 移动\r\n    cube.rotation.x += 0.01; // 旋转\r\n    if (cube.position.x > 5) cube.position.x = 0\r\n    renderer.render(scene, camera);\r\n    //  渲染下一帧的时候就会调用render函数\r\n    requestAnimationFrame(render)\r\n}\r\n\r\nrender()\r\n "},"lineCount":null}},"error":null,"hash":"2a026213b5f0f5c0513b2cbe2a67f65b","cacheData":{"env":{}}}